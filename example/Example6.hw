PRINT"Here is example 6:How many fibonacci numbers do you want?"
INPUT nums
PRINT ""

LET a = 0
LET b = 1
FUNC fibonacci(a, b, nums)
    WHILE nums > 0 REPEAT
        PRINT a
        LET c = a + b
        LET a = b
        LET b = c
        LET nums = nums - 1
    ENDWHILE
    RETURN 0 # It means finished
ENDFUNC

PRINT fibonacci(a, b, nums)
